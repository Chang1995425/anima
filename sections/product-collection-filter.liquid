<style>
  .grid__item .section-header {
  display: none;
}
.image-wrap.product--wrapper img {
    margin: 0 auto;
    text-align: center;
}
.owl-carousel .owl-nav.disabled {
     display: block !important; 
}  
.wrp-related-products .owl-stage-outer{
    height: 530px;
}
.wrp-related-products .owl-dots{
    text-align:center;
}

  
.wrp-related-products .owl-nav .owl-prev{
   left:0;
}
.wrp-related-products .owl-nav .owl-next{
   right:0;
}
  .slick-slider .slick-dots li button{
     text-indent:0px;
  }
.slick-slider .slick-dots li button::before {
   border:1px solid #000;
  
}
.slick-slider .slick-dots li.slick-active button::before {
   background:#000;
}
.slick-dots{
   border-top:3px solid #000;
}
.slick-dots li button {

   border: 0;
  background: transparent;
  display: block;
  height: 81px;
  line-height: 0;
  font-size: 22px;
  color: #000;
  padding: 5px;
  cursor: pointer;
  width: 250px;
}

  .slick-dots li {

    width: auto;
  }
  
  .slick-dots li button::before {
    transform: rotate(45deg);
    border-radius: 0px !important;
    position: absolute;
    top: -17px;
    left: 0;
    content: "\f078";
    width: 26px !important;
height: 26px!important;
    font-family: "FontAwesome";
    font-size: 6px;
    line-height: 20px;
    text-align: center;
    color: #00ce5a;
    opacity: 0.25;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    left: 0 !important;
    right: 0!important;
    margin: 0 auto!important;
}
  
</style>
<script>
$(document).ready(function(){
 
 
  $(".slick-dots li:nth-child(1) button").text("Vetements");
  $(".slick-dots li:nth-child(2) button").text("Tableaux");
  $(".slick-dots li:nth-child(3) button").text("Accessoires");
  $(".slick-dots li:nth-child(4) button").text("Cartes-cadeaux");
  
  
 
  
$('.related-products').owlCarousel({
    nav:true,
    loop:true,
  	slideBy:1,
    responsive:{
        0:{
            items:2,
            margin:20
        },
        600:{
            items:3,
            margin:20
        },
        1000:{
            items:4,
          margin:30
        }
    }
});
  



$('.related-products').on('changed.owl.carousel',function(property){
    var current = property.item.index;
    var itemsAmount = property.item.count;
    console.log(property);
    console.log(current);
    console.log(itemsAmount);
    var webcast;
    if(property.page.size == 2){
    webcast = 2;
    }
    else {
    webcast = 1;
    }
    
    //if (current === itemsAmount+webcast) {
    //console.log(current);
    if(property.page.size == 4 && property.property.value == 7) {
    $('.owl-prev').click(function (){
    	//$(".related-products").trigger('to.owl.carousel', [0, 500]);
    })
  	$(".related-products").trigger('prev.owl.carousel', 500);
  } 
  else if (current === itemsAmount+webcast) {
    $(".related-products").trigger('to.owl.carousel', [0, 500]);
    }
  //}
});
});
  

  
$('.variable-width').not('.slick-initialized').slick({
  slidesToShow: 1,
  slidesToScroll: 1,
  autoplay: false,
  autoplaySpeed: 2000,
  dots: true,
  arrows:false,
  
  draggable: false,
  
    
});
  

</script>
{% schema %}
{
  "name": "Related products",
  "settings": [
    {
      "type": "checkbox",
      "id": "show_related_products",
      "label": "You May Also Like",
      "default": false
    },
    {
      "id": "related_title",
      "type": "text",
      "label": "Section title",
      "default": "You May Also Like"
    },
    {
      "type": "select",
      "id": "related_grid_num",
      "label": "Products per row (Desktop)",
      "default": "4",
      "options": [
        {
          "value": "2",
          "label": "2"
        },
        {
          "value": "3",
          "label": "3"
        },
        {
          "value": "4",
          "label": "4"
        },
        {
          "value": "5",
          "label": "5"
        }
      ]
    },
    {
      "type": "select",
      "id": "related_grid_row",
      "label": "Number of rows (Desktop)",
      "default": "2",
      "options": [
        {
          "value": "1",
          "label": "1"
        },
        {
          "value": "2",
          "label": "2"
        },
        {
          "value": "3",
          "label": "3"
        }
      ]
    }
  ]
}
{% endschema %}
