<div class="wrapper Featured-collection-wrapper test">
<header class="section-header">
        <h2 class="section-header__title">You May Also Like</h2>
      </header>

{% if section.settings.show_related_products == true %}
<div class="product-template__container page-width product-page-related" itemscope itemtype="http://schema.org/Product" id="ProductSection-{{ section.id }}" data-section-id="{{ section.id }}" data-section-type="product" data-enable-history-state="true">
 <aside class="grid">
    <div class="grid__item">
      {% unless heading == blank %}
      <header class="section-header">
        <h2 class="section-header__title">{{ heading }}</h1>
      </header>
      {% endunless %}

      <div class="grid-uniform product-related">
        <div class="owl-carousel related-products aaaaaaaaaa owl-theme" style="margin-bottom: 0;">        
       {% assign number_of_related_products_to_show = 20 %}
{% assign image_size = 'large' %}
{% if product.tags.size > 0 %}
  {% assign counter = '' %}
  {% for prod in collections.all.products %}
          {% for collection in prod.collections %}
                    {% if forloop.first == true %}
              			{% assign col = collection.title | handleize %}
                    {% endif %}
          {% endfor %}
      {% assign new_tag = product.tags | handleize %}
      {% if new_tag contains prod.handle and counter.size < number_of_related_products_to_show %} 
      <div class="item">
          <div class="grid__item grid-product">
       
        <div class="grid-product__wrapper">
         <div data-aos="zoom-out" data-aos-delay="200" class="grid-product__image-wrapper" data-aos="row-of-4">
          <a class="grid-product__image-link" href="{{shop.url}}/collections/{{ col  }}/products/{{ prod.handle }}">
           
          <div class="image-wrap product--wrapper">
            <div>
              {% comment %}
               {{ prod.featured_image | product_img_url: image_size | img_tag }}
              {% endcomment %}
              {% if product.images.size == 1 %} 
                   {{ prod.images[0] | product_img_url: image_size | img_tag }}
              {% else %}
                {{ prod.images[1] | product_img_url: image_size | img_tag }}
              {% endif %}
            </div>
          </div>
          <noscript>
            <img class="grid-product__image" src="//cdn.shopify.com/s/files/1/0004/0809/2719/products/crew_3hd_1024x.jpg?v=1521949750" alt="Tee shirt Homme en V">
          </noscript>
            
            
           </a>
           
           
           </div>
          
          <a href="{{shop.url}}/collections/{{ col  }}/products/{{ prod.handle }}" class="grid-product__meta">
      <span class="grid-product__title">{{ prod.title }}</span>
          <div class="yotpo bottomLine"
  data-product-id="{{ prod.id }}">
</div>
      <span class="grid-product__price-wrap">
        
        <span class="grid-product__price">
          
             <span class="visually-hidden">Regular price</span>
          {{ prod.price | money }}
        </span>
      </span>
      
    </a>
          
          </div>
          </div>
        </div>
      {% endif %}
    {% endfor %}
{% endif %}

        </div>
      </div>
    </div>
  </aside>
  
</div>
{% endif %}

</div>






